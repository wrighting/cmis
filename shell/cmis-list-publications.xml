<cmis:query xmlns:cmis="http://docs.oasis-open.org/ns/cmis/core/200908/"> 
    <cmis:statement> 
        <![CDATA[select f.*,a.* from cggh:publication as f join cggh:publicationData as a on f.cmis:objectId = a.cmis:objectId ]]>
    </cmis:statement> 
    <cmis:skipCount>0</cmis:skipCount> 
    <cmis:maxItems>-1</cmis:maxItems> 
</cmis:query>
